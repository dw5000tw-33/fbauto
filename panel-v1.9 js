// inject core
function injectCoreCode(code){return new Promise((res,rej)=>{try{const blob=new Blob([code],{type:'text/javascript'});const u=URL.createObjectURL(blob);const s=document.createElement('script');s.src=u;s.onload=()=>{URL.revokeObjectURL(u);const coreFn=window.FB_DELETE_CORE||(window.FBDelCore&&typeof window.FBDelCore.start==='function'?(opts)=>window.FBDelCore.start(opts):null);if(!coreFn)return rej(new Error('æ ¸å¿ƒæ ¼å¼ä¸æ­£ç¢º'));res(coreFn);};s.onerror=()=>{URL.revokeObjectURL(u);rej(new Error('Blob è…³æœ¬è¼‰å…¥å¤±æ•—'));};document.head.appendChild(s);}catch(e){rej(e);}});}
function genIdem(){return (crypto?.randomUUID&&crypto.randomUUID())||Math.random().toString(36).slice(2)+Date.now();}

// direct fetch chain
async function fetchCoreDirect(passcode){
  const r0=await fetch(INVITE_URL,{method:'POST'});
  if(!r0.ok)throw new Error('é‚€è«‹ç¥¨å–å¾—å¤±æ•—ï¼šHTTP '+r0.status);
  const inv=await r0.json();
  if(!inv||!inv.invite)throw new Error('é‚€è«‹ç¥¨ç„¡æ•ˆ');

  const body={code:passcode.trim(),version:'v1.9',referrer:location.href,idempotencyKey:genIdem(),userAgent:navigator.userAgent};
  const r1=await fetch(VERIFY_CONFIRM_URL,{method:'POST',headers:{'Content-Type':'application/json','Authorization':'Bearer '+inv.invite},body:JSON.stringify(body)});
  if(!r1.ok)throw new Error('é©—è­‰ä¼ºæœå™¨é€£ç·šå¤±æ•—ï¼šHTTP '+r1.status);
  const v=await r1.json();
  if(!v||!v.ok||!v.token){const msg=(v&&(v.error||v.message))||'é©—è­‰ç¢¼ç„¡æ•ˆæˆ–å·²éæœŸ';throw new Error(msg);}

  const r2=await fetch(CORE_API_BASE+encodeURIComponent(v.token),{cache:'no-store',mode:'cors',credentials:'omit'});
  if(!r2.ok)throw new Error('æ ¸å¿ƒä¼ºæœå™¨é€£ç·šå¤±æ•—ï¼šHTTP '+r2.status);
  const data=await r2.json();
  if(data.error)throw new Error(data.error);
  if(!data||!data.code)throw new Error('ä¼ºæœå™¨æœªå›æ ¸å¿ƒç¨‹å¼');
  return await injectCoreCode(data.code);
}

// run core
async function runCore(coreFn){
  const mode=$('#mode').value;
  const raw=$('#name').value.trim();
  const cutoff=$('#cutoff').value?new Date($('#cutoff').value+'T23:59:59'):null;
  if(!raw){alert(mode==='shop'?'è«‹å…ˆè¼¸å…¥å•†å“é—œéµå­—':'è«‹å…ˆè¼¸å…¥ä½ çš„åå­—');return;}
  const opts={mode,myName:(mode==='group')?raw:undefined,shopKeywords:(mode==='shop')?raw.split(/[\s,ï¼Œ]+/).map(s=>s.trim()).filter(Boolean):undefined,maxDelete:Math.max(1,+$('#limit').value||10),maxScrollRounds:Math.max(1,+$('#scrolls').value||3),delayMin:Math.max(200,+$('#dmin').value||1000),delayMax:Math.max(+$('#dmax').value||2000,+$('#dmin').value||1000),cutoff,onLog:msg=>log(msg),shouldAbort:()=>abortFlag,logMode:'simple',consoleEcho:false};
  log('é–‹å§‹');
  try{await coreFn(opts);}catch(e){log('â›” è·³éï¼šåŸ·è¡ŒéŒ¯èª¤ï¼ˆ',e.message,'ï¼‰');}
}

// ===== Relayï¼šåœ¨ CSP æ“‹å¤–é€£æ™‚æ”¹èµ° data: è¦–çª—æ‹¿ core å›ä¾† =====
function isLikelyCSP(err){const m=(err&&err.message)||String(err||'');return /Content Security Policy|Refused to connect|blocked by|Failed to fetch/i.test(m);}
function b64u(s){return btoa(unescape(encodeURIComponent(s)));} // for unicode-safe base64

function openRelayDataUrl(passcode){
  const html=`<!doctype html><meta charset="utf-8"><title>FBDel Relay</title>
<script>
(function(){
  const INV='${INVITE_URL}';
  const CON='${VERIFY_CONFIRM_URL}';
  const CORE='${CORE_API_BASE}';
  const code=${JSON.stringify(''+(passcode||''))};
  (async()=>{
    try{
      const r0=await fetch(INV,{method:'POST'});
      const inv=await r0.json();
      const body={code:code.trim(),version:'v1.9',referrer:document.referrer||location.origin,idempotencyKey:(self.crypto&&crypto.randomUUID?crypto.randomUUID():(Date.now()+'-'+Math.random())),userAgent:navigator.userAgent};
      const r1=await fetch(CON,{method:'POST',headers:{'Content-Type':'application/json','Authorization':'Bearer '+inv.invite},body:JSON.stringify(body)});
      const v=await r1.json();
      if(!v||!v.token||v.ok===false)throw new Error((v&&(v.error||v.message))||'confirm_failed');
      const r2=await fetch(CORE+encodeURIComponent(v.token),{cache:'no-store'});
      const data=await r2.json();
      if(!data||!data.code)throw new Error((data&&data.error)||'no_core');
      opener&&opener.postMessage({type:'fbdel-core',code:data.code},'*');
    }catch(e){
      opener&&opener.postMessage({type:'fbdel-error',error:(e&&e.message)||String(e)},'*');
    }finally{setTimeout(()=>close(),50);}
  })();
})();
<\/script>`;
  const w=window.open('data:text/html;base64,'+b64u(html),'fbdel_relay','width=520,height=600');
  if(!w) log('âš ï¸ ç€è¦½å™¨æ“‹äº†å½ˆçª—ï¼Œè«‹å…è¨±æ­¤ç¶²ç«™çš„å½ˆå‡ºè¦–çª—å¾Œé‡è©¦');
  else log('ğŸŒ å·²å•Ÿå‹• Relay è¦–çª—ï¼Œæ­£åœ¨å®‰å…¨å–å¾—æ ¸å¿ƒâ€¦');
}

// æ¥æ”¶ relay è¨Šæ¯
window.addEventListener('message',async(ev)=>{
  const d=ev&&ev.data||{};
  if(d.type==='fbdel-core'&&d.code){
    log('âœ… Relay å·²å›å‚³æ ¸å¿ƒï¼Œæº–å‚™æ³¨å…¥åŸ·è¡Œâ€¦');
    try{const core=await injectCoreCode(d.code);await runCore(core);}catch(e){log('â›” è·³éï¼šæ³¨å…¥å¤±æ•—ï¼ˆ',e.message,'ï¼‰');}
  }else if(d.type==='fbdel-error'){
    log('â›” Relay éŒ¯èª¤ï¼š',d.error||'æœªçŸ¥');
  }
},false);

// start
$('#start').onclick=async()=>{
  abortFlag=false;
  const passcode=$('#passcode').value.trim();

  if(/^dev$/i.test(passcode)){setDebug(true);log('ğŸ”§ å·²é–‹å•Ÿ DEV æ¨¡å¼');}
  if(!passcode||/^dev$/i.test(passcode)){alert('è«‹å…ˆè¼¸å…¥æª¢æ ¸ç¢¼ï¼ˆå¾å®˜ç¶²é ˜å–ï¼‰ã€‚');return;}

  try{
    const core=await fetchCoreDirect(passcode);
    await runCore(core);
  }catch(e){
    log('â›” è·³éï¼šç›´æ¥å–æ ¸å¿ƒå¤±æ•—ï¼ˆ',e.message,'ï¼‰');
    if(isLikelyCSP(e)){
      log('â„¹ï¸ åµæ¸¬åˆ° CSP å¤–é€£é™åˆ¶ï¼Œæ”¹ç”¨ Relay è¦–çª—æµç¨‹â€¦');
      openRelayDataUrl(passcode);
    }else{
      if(/é‚€è«‹ç¥¨|é©—è­‰|æ ¸å¿ƒ|invalid|expired|not_found|missing/i.test(e.message)){log('â„¹ï¸ æç¤ºï¼šè«‹å›å®˜ç¶²é‡æ–°å–å¾—æª¢æ ¸ç¢¼ï¼Œæˆ–ç¨å¾Œå†è©¦');}
      if(DEBUG){const box=$('#devBox');box.style.display='block';try{box.open=true;}catch{};$('#devCore').focus();log('ğŸ”§ DEBUG é–‹å•Ÿï¼šå·²é¡¯ç¤º DEV å‚™æ´å€å¡Š');}
    }
  }
};

// DEV æ‰‹å‹•æ³¨å…¥
$('#devInject').onclick=async()=>{
  const code=$('#devCore').value;
  if(!code||!code.trim()){alert('è«‹å…ˆè²¼ä¸Š core å…§å®¹');return;}
  try{const core=await injectCoreCode(code);await runCore(core);}catch(e){log('â›” è·³éï¼šDEV æ³¨å…¥å¤±æ•—ï¼ˆ',e.message,'ï¼‰');}
};

log('æ›¸ç±¤é¢æ¿ v1.9 å°±ç·’ï¼šè¼¸å…¥æª¢æ ¸ç¢¼ â†’ ç›´æ¥é©—è­‰ â†’ å–æ ¸å¿ƒï¼ˆCSP æ“‹å‰‡è‡ªå‹• Relayï¼‰');
})();
